{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        ".*requirements\\.txt$"
      ],
      "matchStrings": [
        "git\\+https://github.com/(?<orgName>[^/]+)/(?<depName>[^@]+)\\.git@v(?<currentValue>[0-9]+\\.[0-9]+\\.[0-9]+)-fertiscan-datastore"
      ],
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "{{{orgName}}}/{{{depName}}}"
    }
  ],
  "packageRules": [
    {
      "matchDatasources": ["github-tags"],
      "matchPackageNames": ["ai-cfia/ailab-datastore"],
      "versioning": "regex:^(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)-fertiscan-datastore$"
    }
  ]
}
