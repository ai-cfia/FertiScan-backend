{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        ".*requirements\\.txt$"
      ],
      "matchStrings": [
        "git\\+https://github.com/(?<orgName>[^/]+)/(?<depName>[^@]+)\\.git@(?<currentValue>v[0-9]+\\.[0-9]+\\.[0-9]+-fertiscan-datastore)"
      ],
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "{{{orgName}}}/{{{depName}}}",
      "extractVersionTemplate": "{{{currentValue}}}"
    }
  ]
}
